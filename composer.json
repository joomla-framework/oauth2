{
    "name": "joomla/oauth2",
    "type": "joomla-package",
    "description": "Joomla OAuth2 Package",
    "keywords": ["joomla", "framework", "oauth2"],
    "homepage": "https://github.com/joomla-framework/oauth2",
    "license": "GPL-2.0-or-later",
    "require": {
        "php": "^8.1.0",
        "joomla/application": "3.0.x-dev",
        "joomla/http": "3.0.x-dev",
        "joomla/input": "3.0.x-dev",
        "joomla/session": "3.0.x-dev",
        "joomla/uri": "3.0.x-dev"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5.28",
        "squizlabs/php_codesniffer": "^3.7.2",
        "phpstan/phpstan": "^1.10.7",
        "phan/phan": "^5.4.2"
    },
    "autoload": {
        "psr-4": {
            "Joomla\\OAuth2\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Joomla\\OAuth2\\Tests\\": "Tests/"
        }
    },
    "minimum-stability": "dev",
    "extra": {
        "branch-alias": {
            "dev-2.0-dev": "2.0-dev",
            "dev-3.x-dev": "3.0-dev"
        }
    }
}
